<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".KetList">



        <TextView
            android:id="@+id/hsl_user_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#FFF"
            android:text="user"
            android:layout_marginStart="230dp"
            />

        <TextView
            android:id="@+id/idep"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#FFF"
            android:text="id"
            android:layout_marginStart="280dp"
            />




    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >


     <!--   <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardElevation="10dp"
            android:background="#fff"
            android:layout_margin="2dp"
            app:cardCornerRadius="3dp">-->




            <RelativeLayout
                android:id="@+id/jh1"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/hasilsp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#fff"
                    android:text="this"
                    android:textAlignment="center"
                    android:layout_marginTop="1dp"/>







                <RelativeLayout
                    android:id="@+id/jhhh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/hasilsp">









                    <LinearLayout
                        android:id="@+id/liu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginStart="15dp"
                        android:layout_toRightOf="@+id/lid"
                        android:orientation="horizontal">


                        <TextView
                            android:id="@+id/u"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/lid"
                            android:textColor="#000000"
                            android:text="Nama Pasien : "/>

                        <TextView
                            android:id="@+id/user_riwayat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:hint="Pasien"
                            android:layout_marginStart="3dp"
                            android:layout_toRightOf="@+id/u"
                            />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/hsltgl"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_below="@+id/liu"
                        android:layout_toRightOf="@+id/lid"
                        android:orientation="horizontal">


                        <TextView
                            android:id="@+id/jk_riwayat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/liu"
                            android:textColor="#000000"
                            android:text="Jenis Kelamin"
                            />


                        <TextView
                            android:id="@+id/mau"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/liu"
                            android:textColor="#000000"
                            android:layout_toRightOf="@+id/jk"
                            android:text=" - Lahir, "
                            />

                        <TextView
                            android:id="@+id/tgl_lhr_riwayat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="3dp"
                            android:textColor="#000000"
                            android:layout_toRightOf="@+id/mau"
                            android:hint="-"
                            />

                    </LinearLayout>



                    <LinearLayout
                        android:id="@+id/tgl_diag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/hsltgl"
                        android:layout_marginStart="15dp"
                        android:layout_toRightOf="@+id/lid"
                        android:orientation="horizontal">


                        <TextView
                            android:id="@+id/t"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="Tanggal Diagnosa, "/>

                        <TextView
                            android:id="@+id/tgl_diagnosa_riwayat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:hint="Pasien"
                            android:layout_marginStart="3dp"
                            android:layout_toRightOf="@+id/u"
                            />

                    </LinearLayout>

                </RelativeLayout>


                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@+id/jhhh"
                    android:layout_marginStart="35dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="35dp"
                    android:background="@android:color/black" />

                <View
                    android:id="@+id/view3"
                    android:background="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="95dp"
                    android:layout_marginEnd="95dp"
                    android:layout_below="@+id/view2"
                    android:layout_height="2dp"/>





                <TextView
                    android:id="@+id/txta"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="80dp"
                    android:layout_marginEnd="80dp"
                    android:textColor="#000000"
                    android:layout_below="@+id/view3"
                    android:text="Dari Indikasi Yang Di Dapatkan, Anda Disarankan Untuk :"
                    android:textAlignment="center"
                    android:layout_marginTop="10dp"/>




                <TextView
                    android:id="@+id/perawatan_riwayat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txta"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="20dp"
                    android:text="Perawatan"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <RelativeLayout
                    android:id="@+id/xx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:layout_marginEnd="25dp"
                    android:layout_below="@+id/perawatan_riwayat"
                    android:layout_marginTop="5dp"
                    android:gravity="center_horizontal"
                    >


                    <TextView
                        android:id="@+id/kd1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kode : "
                        android:textColor="#000000"
                        />

                    <TextView
                        android:id="@+id/kdpg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:hint="kode"
                        android:layout_toRightOf="@+id/kd1"
                        android:layout_marginStart="3dp"
                        />

                </RelativeLayout>





                <ImageView
                    android:id="@+id/image_hslr"
                    android:layout_width="match_parent"
                    android:layout_height="165dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="50dp"
                    android:layout_marginEnd="50dp"
                    android:src="@drawable/box"
                    android:layout_below="@+id/xx"
                    />







                <View
                    android:id="@+id/view"
                    android:background="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="35dp"
                    android:layout_marginEnd="35dp"
                    android:layout_below="@+id/image_hslr"
                    android:layout_height="2dp"/>



                <TextView
                    android:id="@+id/perawatan_riwayat2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view"
                    android:layout_marginStart="35dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:text="Perawatan"
                    android:textColor="#000000"
                    />



                <RelativeLayout
                    android:id="@+id/r_ket"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/perawatan_riwayat2"
                    android:background="#BAE6EC"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp">

                    <TextView
                        android:id="@+id/keterangan_riwayat1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:hint="Keterangan"
                        android:justificationMode="inter_word"
                        android:textColor="#000000"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"/>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/r_ket2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/r_ket"
                    android:background="#BAE6EC"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginEnd="20dp">

                    <TextView
                        android:id="@+id/keterangan_riwayat2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:hint="-"
                        android:justificationMode="inter_word"
                        android:textColor="#000000"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"/>

                </RelativeLayout>


                <Button
                    android:id="@+id/pdf_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Get Pdf"
                    android:background="@drawable/button_custom1"
                    android:textSize="14sp"
                    android:layout_marginTop="15dp"
                    android:layout_below="@+id/r_ket2"
                    android:layout_marginBottom="10dp"
                    android:layout_marginStart="125dp"
                    android:layout_marginEnd="125dp"/>



            </RelativeLayout>



    <!--    </androidx.cardview.widget.CardView>-->

    </ScrollView>


</RelativeLayout>
