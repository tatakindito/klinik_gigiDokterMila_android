                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#fff"
                android:text="user"
                android:layout_marginStart="30dp"
                />

        </LinearLayout>






    <LinearLayout
        android:id="@+id/liu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginStart="15dp"
        android:layout_toRightOf="@+id/lid"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/u"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/lid"
            android:textColor="#000000"
            android:text="Nama Pasien : "/>

        <TextView
            android:id="@+id/user_sp_open"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:hint="Pasien"
            android:layout_marginStart="3dp"
            android:layout_toRightOf="@+id/u"
            />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/hsltgl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_below="@+id/liu"
        android:layout_toRightOf="@+id/lid"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/jk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/liu"
            android:textColor="#000000"
            android:text="Jenis Kelamin"
            />


        <TextView
            android:id="@+id/mau"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/liu"
            android:textColor="#000000"
            android:layout_toRightOf="@+id/jk"
            android:text=" - Lahir, "
            />

        <TextView
            android:id="@+id/tgl_lhr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:textColor="#000000"
            android:layout_toRightOf="@+id/mau"
            android:hint="-"
            />

    </LinearLayout>



    <LinearLayout
        android:id="@+id/tgl_diag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/hsltgl"
        android:layout_marginStart="15dp"
        android:layout_toRightOf="@+id/lid"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Tanggal Diagnosa, "/>

        <TextView
            android:id="@+id/tgl_diagnosa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:hint="Pasien"
            android:layout_marginStart="3dp"
            android:layout_toRightOf="@+id/u"
            />

    </LinearLayout>

        </RelativeLayout>











